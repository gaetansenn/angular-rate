{
  "env": {
    "amd": true,
    "node": true
  },
  "plugins": [
    "angular"
  ],
  "rules": {
    "eqeqeq": 2,
    "quotes": [0, "single"],
    "strict": 0,
    "curly": [2, "multi-line"],
    "eol-last": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "no-loop-func": 0,
    "angular/ng_on_watch": 0,
    "angular/ng_no_services": 0,
    "angular/ng_controller_as": 0,
    "angular/ng_controller_as_route": 0,
    "angular/ng_controller_as_vm": 0
  },
  "globals": {
    "angular": true,
    "app": true,
    "_": true
  }
}
